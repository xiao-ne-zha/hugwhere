where-clause = WHERE conds / conds
conds = (cond | elements)+
cond = <LB> elements <RB>
<elements> = (element | cond)+
<element> = STR | KID | DCOLON | ANY
KID = #"((?<!\\):[-\w]+\*?)?(?<!\\):[-\w]+(\.[-\w]+)*"
<LB> = '[['
<RB> = ']]'
STR = SQUOTA_STR | DQUOTA_STR
<SQUOTA_STR> = '"' ( #'\\.' | '""' | #'[^"\\]' )* '"'
<DQUOTA_STR> = "'" ( #"\\." | "''" | #"[^'\\]" )* "'"
<DCOLON> = #"::[\w-]+"
<ANY> = #"(?!((\[\[)|(\]\])))([^:\s'\"\(][^\s]*)|(\([^:\s]*)"
<WHERE> = 'where' | 'WHERE'
