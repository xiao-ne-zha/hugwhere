where-clause = WHERE conds / conds
conds = (cond | elements)+
cond = <LB> elements <RB>
<elements> = (element | cond)+
<element> = (STR / KID / ANY)
KID = #"((?<!\\):[-\w]+\*?)?(?<!\\):[-\w]+(\.[-\w]+)*"
<LB> = '[['
<RB> = ']]'
<STR> = #"'.+?'" | #'".+?"'
<ANY> = #"([^:\s'\"\(][^\s]*)|(\([^:\s]*)"
<WHERE> = 'where' | 'WHERE'
